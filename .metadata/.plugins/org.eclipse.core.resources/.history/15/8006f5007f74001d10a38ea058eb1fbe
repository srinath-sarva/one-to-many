package com.sarva.DemoMaven;

public class EmployeeBean {

	

	private int empId;
	private String empName;
	private AddressBean addr;
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
	public AddressBean getAddr() {
		return addr;
	}
	public void setAddr(AddressBean addr) {
		this.addr = addr;
	}
	public EmployeeBean() {
		super();
	}
	
	public void printEmployeeBean() {
		System.out.println("EmployeeBean [empId=" + empId + ", empName=" + empName + ", addr=" + addr + "]"); 
		System.out.println(addr.toString());
	}
	
	
}
